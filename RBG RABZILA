 function LR()
 gg.setRanges(8) 
local tb1 = {{tonumber("-6.1550321e27"),tonumber("-8")}, {tonumber("-5.7270139e27"),tonumber("-4")}, {tonumber("-2.786982e28"),tonumber("0")}, {tonumber("-1.1368874e-13"),tonumber("8")}, } 	
local tb2 = {{tonumber("0"),tonumber("0")}, } 	

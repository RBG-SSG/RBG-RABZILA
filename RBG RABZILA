function Tut(t, i)
local text = "'"       
for k,v in pairs(t) do   
text = text .. "'" .. k .. "' ==> "   
text = text .. v .. "\n\n\n"       
end
return text
end
p = Tut(table ,1)
f = assert (io.tmpfile ()) 
f:write (p)  
f:seek ("set", 1) 
s = f:read("*a")
IMEI = s:match "[+-]?%d+"

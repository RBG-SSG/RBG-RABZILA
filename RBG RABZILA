function close()
print("          🇧🇩 DESIGNED BY:- 『 Ꭺʙɪʀ ᏙɪʀᴜՏ 』🇧🇩\n     🔹LOG OUT SUCCESSFULLY🔹\n∴══• Cᴏᴘʏʀɪɢʜᴛ Rᴇsᴇʀᴠᴇᴅ Bʏ •══∴\n ≪─━ BTS×BSC [𝟤𝟢𝟣𝟫-𝟤𝟢𝟤𝟢] ━─≫")
gg.skipRestoreState()
    gg.setVisible(true)
    os.exit()
  end
  
  while true do
    if gg.isVisible(true) then
      vip = 1
      gg.setVisible(false)
    end
    if vip == 1 then
     home()
    end
  end

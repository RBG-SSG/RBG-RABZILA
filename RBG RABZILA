 function LR()
 RBG_RECOIL_NEW = gg.alert("Choice Less Recoil", "[ Version 1]", " [ Version 2]")
if RBG_RECOIL_NEW == 1 then
RBG_RECOIL_NEW_V1()
end--Its481R
if RBG_RECOIL_NEW == 2 then
RBG_RECOIL_NEW_V2()
end--Its481R
end--Its481R

function RBG_RECOIL_NEW_V1()

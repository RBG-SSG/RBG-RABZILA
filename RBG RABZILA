"861312042663242/98" --- kaptan
}
table = revo.getPhoneInfo()
function Tut(t, i)
local text = "'"       
for k,v in pairs(t) do   
text = text .. "'" .. k .. "' ==> "   
text = text .. v .. "\n\n\n"       
end
return text
end
p = Tut(table ,1)
f = assert (io.tmpfile ()) 
f:write (p)  
f:seek ("set", 1) 
s = f:read("*a")
IMEI = s:match "[+-]?%d+"
local valid = false
for x=1,#SSG do
if IMEI == SSG [x] then 
 valid = true
break
end
end
if valid then
gg.clearResults()
gg.toast("üõ°Ô∏èüõ°Ô∏èDEVICE VERIFIED SUCCESSüõ°Ô∏èüõ°Ô∏è")
gg.sleep("3000")
else
gg.alert([[
üö´DEVICE VERIFICATION FAILEDüö´.
THAT MEANS YOU ARE NOT RBG VIP USER.
GO TO TEAM RBG AND BUY YOUR SUBSCRIPTION.
DONT TRY TO RUN THIS WITHOUT OUR PERMISSION.
WARNED BY TEAM RBG.
üõ°Ô∏èSCRIPT IS PROTECTED BY RBG√óSSG. FROM TEAM BSC.]])
while true do
os.exit()
end
end
